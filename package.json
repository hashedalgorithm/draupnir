    "husky": "^9.0.11",
    "postcss": "^8.4.38",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^11.1.2",
    "tailwindcss": "^3.4.3",
